<p drab-commander="DrabPoc.Timer4Commander" drab-argument="{seconds: 10}">
  <button class="btn btn-primary" drab-click="countdown">
    Count down from 10
  </button>
  <br><span><%= @countdown %></span>
</p>
<p drab-commander="DrabPoc.Timer4Commander" drab-argument="{seconds: 20}">
  <br>
  <button class="btn btn-primary" drab-click="countdown">
    Count down from 20
  </button>
  <br><span><%= @countdown %></span>
</p>
